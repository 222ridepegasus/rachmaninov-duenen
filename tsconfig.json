{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@components/structure/*": ["src/components/structure/*"],
      "@components/case-study/*": ["src/components/case-study/*"],
      "@styles/*": ["src/styles/*"]
    },
    "types": ["astro/client"]
  },
  "include": [".astro/types.d.ts", "src/**/*", "astro.config.mjs"],
  "exclude": ["dist"]
}
